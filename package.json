{
  "name": "react-chat-app",
  "main": "lib/bundle",
  "version": "0.1.0",
  "description": "A UI library of react for online chat application",
  "repository": "https://github.com/Yaozu-Xu/react-chat-app",
  "keywords": [
    "react",
    "chat",
    "reactjs",
    "ui",
    "vite"
